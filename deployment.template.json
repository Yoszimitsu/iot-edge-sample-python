{
  "$schema-template": "1.0.0",
  "modulesContent": {
    "$edgeAgent": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "runtime": {
          "type": "docker",
          "settings": {
            "minDockerVersion": "v1.25",
            "loggingOptions": "",
            "registryCredentials": {
              "YourACR": {
                "username": "$CONTAINER_REGISTRY_USERNAME_iotcontainerregister",
                "password": "$CONTAINER_REGISTRY_PASSWORD_iotcontainerregister",
                "address": "$DEVOPS_IOTEDGE_REGISTRY_URL"
              }
            }
          }
        },
        "systemModules": {
          "edgeAgent": {
            "type": "docker",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-agent:1.0",
              "createOptions": ""
            }
          },
          "edgeHub": {
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-hub:1.0",
              "createOptions": "{\"HostConfig\":{\"PortBindings\":{\"5671/tcp\":[{\"HostPort\":\"5671\"}], \"8883/tcp\":[{\"HostPort\":\"8883\"}],\"443/tcp\":[{\"HostPort\":\"443\"}]}}}"
            }
          }
        },
        "modules": {
          "SampleModule": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.SampleModule}",
              "createOptions": ""
            }
          },
          "moxa_demokit_streamAnalytics_DanielM": {
            "settings": {
                "image": "mcr.microsoft.com/azure-stream-analytics/azureiotedge:1.0.10",
                "createOptions": ""
            },
            "type": "docker",
            "version": "1.0.0.0",
            "env": {
                "PlanId": {
                    "value": "stream-analytics-on-iot-edge"
                }
            },
            "status": "running",
            "restartPolicy": "always"
          }
        }
      }
    },
    "$edgeHub": {
      "properties.desired": {
        "schemaVersion": "1.1",
        "routes": {
          "knob2ToCloud": {
            "route": "FROM /messages/modules/SampleModule/outputs/knob2 INTO $upstream",
            "priority": 0
          },
          "resetToMachine": {
            "route": "FROM /messages/modules/moxa_demokit_streamAnalytics_DanielM/* INTO BrokeredEndpoint(\"/modules/SampleModule/inputs/control\")",
            "priority": 0
          },
          "temperatureToAsa": {
            "route": "FROM /messages/modules/SampleModule/outputs/temperature INTO BrokeredEndpoint(\"/modules/moxa_demokit_streamAnalytics_DanielM/inputs/temperature\")",
            "priority": 1
          },
          "alertsToCloud": {
              "route": "FROM /messages/modules/moxa_demokit_streamAnalytics_DanielM/* INTO $upstream)",
              "priority": 1
          }
        },
        "storeAndForwardConfiguration": {
          "timeToLiveSecs": 7200
        }
      }
    },
    "SampleModule": {
      "properties.desired": {}
    },
    "moxa_demokit_streamAnalytics_DanielM": {
      "properties.desired": {
        "ASAJobInfo": "https://moxademokitdanielm.blob.core.windows.net/asa-edge-5512ad4b-69ef-44a0-82d1-c4af8a7fe169/ASAEdgeJobs/5512ad4b-69ef-44a0-82d1-c4af8a7fe169/3d580ab4-9f62-4949-a8ed-eb2b5c7666de/ASAEdgeJobDefinition.zip?sv=2018-03-28&sr=b&sig=bMjE34LXoroG69frZvmrvOInZwq9nKN%2Fe%2B1tFps9FgQ%3D&st=2021-12-07T15%3A26%3A56Z&se=2024-12-07T15%3A36%3A56Z&sp=r",
        "ASAJobResourceId": "/subscriptions/95ed7aca-37c4-45ce-98bf-d849e5cd59f5/resourceGroups/IOTdemo/providers/Microsoft.StreamAnalytics/streamingjobs/moxa_demokit_streamAnalytics_DanielM",
        "ASAJobEtag": "4f200749-de94-49e6-8846-fb7f1118d160",
        "PublishTimestamp": "12/7/2021 3:36:56 PM"
      }
    }
  }
}
